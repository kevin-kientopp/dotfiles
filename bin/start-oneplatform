#!/usr/bin/env bash

start-oracle-listener > /tmp/start-oracle-listener.log 2>&1
oneplatform-cloud-discovery > /var/log/zion/oneplatform-cloud-discovery.log 2>&1 &
sleep 2; oneplatform-cloud-config > /var/log/zion/oneplatform-cloud-config.log 2>&1 &
until { netstat -tupln | grep 8762; } >/dev/null 2>&1; do
  sleep 5 
done 
sleep 20
oneplatform-cloud-apigateway > /var/log/zion/oneplatform-cloud-apigateway.log 2>&1
